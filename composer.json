{
    "name": "tamil/test-package",
    "description": "A production-ready demo PHP package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tamil\\TestPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tamil\\TestPackage\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tamil",
            "email": "email@example.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
